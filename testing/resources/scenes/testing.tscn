resource_name = "fps_label"
script/source = "extends Label


func _enter_tree() -> void:
	hide()
	DebugConsole.add_console_command(\"show_fps\", show_fps, TYPE_NIL)


func _process(_delta: float) -> void:
	if not visible:
		return

	text = \"%d FPS\"%Engine.get_frames_per_second()


func _exit_tree() -> void:
	DebugConsole.remove_console_command(\"show_fps\")


func show_fps() -> void:
	visible = not visible
"

