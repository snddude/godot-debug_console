resource_name = "camera_3d"
script/source = "extends Camera3D


func _enter_tree() -> void:
	DebugConsole.add_console_command(\"unshaded\", unshaded, TYPE_NIL)
	DebugConsole.add_console_command(\"wireframe\", wireframe, TYPE_NIL)
	DebugConsole.add_console_command(\"overdraw\", overdraw, TYPE_NIL)


func _exit_tree() -> void:
	DebugConsole.remove_console_command(\"unshaded\")
	DebugConsole.remove_console_command(\"wireframe\")
	DebugConsole.remove_console_command(\"overdraw\")


func unshaded() -> void:
	if get_viewport().debug_draw == Viewport.DEBUG_DRAW_DISABLED:
		get_viewport().debug_draw = Viewport.DEBUG_DRAW_UNSHADED
		return

	get_viewport().debug_draw = Viewport.DEBUG_DRAW_DISABLED


func wireframe() -> void:
	if get_viewport().debug_draw == Viewport.DEBUG_DRAW_DISABLED:
		get_viewport().debug_draw = Viewport.DEBUG_DRAW_WIREFRAME
		return

	get_viewport().debug_draw = Viewport.DEBUG_DRAW_DISABLED


func overdraw() -> void:
	if get_viewport().debug_draw == Viewport.DEBUG_DRAW_DISABLED:
		get_viewport().debug_draw = Viewport.DEBUG_DRAW_OVERDRAW
		return

	get_viewport().debug_draw = Viewport.DEBUG_DRAW_DISABLED
"

